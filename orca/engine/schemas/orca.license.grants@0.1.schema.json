{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Orca License Grants",
    "type": "object",
    "properties": {
        "grants": {
            "type": "array",
            "items": {
                "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "sku": {
                                "type": "string"
                            },
                            "granted_at": {
                                "type": "string"
                            },
                            "granted_by": {
                                "type": "string"
                            },
                            "activation_source": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "sku"
                        ]
                    }
                ]
            }
        }
    },
    "required": [
        "grants"
    ]
}